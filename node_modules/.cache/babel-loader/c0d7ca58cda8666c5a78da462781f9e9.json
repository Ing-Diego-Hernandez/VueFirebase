ss=\"modal-dialog\">\n    <div class=\"modal-content\">\n      <div class=\"modal-header\">\n        <h5 class=\"modal-title\">Regístrate</h5>\n        <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n      </div>\n      <div class=\"modal-body\">\n        <form @submit.prevent=\"register( this.email, this.password )\">  \n           <!-- CORREO -->\n          <div class=\"input-group mb-3\">\n          <span class=\"input-group-text\">Correo</span>\n          <input v-model=\"email\" \n                  type=\"email\"\n                  required=\"true\"\n                  class=\"form-control\">\n          </div>\n          <!-- PASSWORD -->\n          <div class=\"input-group mb-3\">\n          <span class=\"input-group-text\">Password</span>\n          <input v-model=\"password\" \n                  type=\"password\"\n                  required=\"true\" \n                  class=\"form-control\">\n          </div>\n          <!-- REPASSWORD -->\n          <div class=\"input-group mb-3\">\n          <span class=\"input-group-text\">Repite Password</span>\n          <input v-model=\"repassword\" \n                  type=\"password\"\n                  required=\"true\" \n                  class=\"form-control\">\n          </div>\n          <div class=\"d-grid gap-2\">\n            <button type=\"submit\" class=\"btn btn-primary\" data-bs-dismiss=\"modal\">Registrar</button>\n          </div>\n        </form>\n      </div>\n    </div>\n  </div>\n</div>\n<!-- //// Modal - Iniciar sesión //// -->\n<div class=\"modal fade\" id=\"login\">\n  <div class=\"modal-dialog\">\n    <div class=\"modal-content\">\n      <div class=\"modal-header\">\n        <h5 class=\"modal-title\">Inicia de sesión</h5>\n        <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n      </div>\n      <div class=\"modal-body\">\n        <form @submit.prevent=\"login( this.email, this.password )\">\n          <!-- CORREO --> \n          <div class=\"input-group mb-3\">\n          <span class=\"input-group-text\">Correo</span>\n          <input v-model=\"email\" \n                  type=\"email\"\n                  required=\"true\"\n                  class=\"form-control\">\n          </div>\n          <!-- PASSWORD --> \n          <div class=\"input-group mb-3\">\n          <span class=\"input-group-text\">Password</span>\n          <input v-model=\"password\" \n                  type=\"password\"\n                  required=\"true\" \n                  class=\"form-control\">\n          </div>\n          <div class=\"d-grid gap-2\">\n            <button type=\"submit\" \n              class=\"btn btn-primary\" \n              data-bs-dismiss=\"modal\"><!-- Cierra el modal --> \n              Iniciar sesión\n            </button>\n          </div>\n          </form>\n      </div>\n    </div>\n  </div>\n</div>\n</template>\n\n<script>\nimport { \n  getAuth,\n  signOut,\n  createUserWithEmailAndPassword,\n  signInWithEmailAndPassword\n  } from \"firebase/auth\";\n  import { mapGetters } from 'vuex'\n\nexport default {\n  name: 'Navbar',\n   data() {\n        return {\n            email: '',\n            password: '',\n            repassword: '',\n            errorMessage: ''\n        };\n    },\n   methods: {\n     register(email, password) {\n        const auth = getAuth();\n        createUserWithEmailAndPassword(auth, email, password)\n        .then((userCredential) => {\n            const user = userCredential.user;\n            alert('¡Registrado!');\n        })\n        .catch((error) => {\n            const errorCode = error.code;\n            this.errorMessage = error.message;\n            alert(this.errorMessage);\n            // ..\n        });\n      },        \n     login( email, password ) {\n       const auth = getAuth();\n        signInWithEmailAndPassword(auth, email, password)\n        .then((userCredential) => {\n            alert('¡Sesión iniciada!');\n            // Signed in\n            const user = userCredential.user;\n            // ...\n            })\n        .catch((error) => {\n        const errorCode = error.code;\n        this.errorMessage = error.message;\n        alert(this.errorMessage);\n        });\n        },\n     signout () {\n       const auth = getAuth();\n      signOut(auth).then(() => {\n        alert('¡Sesión cerrada! Inicia sesión.');\n      }).catch((error) => {\n      });\n     }\n   },\n    computed: {\n    ...mapGetters(['existeUsuario'])\n  }\n}\n</script>\n"],"mappings":"AA8HA,SACEA,OAAO,EACPC,OAAO,EACPC,8BAA8B,EAC9BC,0BAAyB,QAClB,eAAe;AACtB,SAASC,UAAS,QAAS,MAAK;AAElC,eAAe;EACbC,IAAI,EAAE,QAAQ;EACbC,IAAIA,CAAA,EAAG;IACF,OAAO;MACHC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE,EAAE;MACdC,YAAY,EAAE;IAClB,CAAC;EACL,CAAC;EACFC,OAAO,EAAE;IACPC,QAAQA,CAACL,KAAK,EAAEC,QAAQ,EAAE;MACvB,MAAMK,IAAG,GAAIb,OAAO,CAAC,CAAC;MACtBE,8BAA8B,CAACW,IAAI,EAAEN,KAAK,EAAEC,QAAQ,EACnDM,IAAI,CAAEC,cAAc,IAAK;QACtB,MAAMC,IAAG,GAAID,cAAc,CAACC,IAAI;QAChCC,KAAK,CAAC,cAAc,CAAC;MACzB,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;QACd,MAAMC,SAAQ,GAAID,KAAK,CAACE,IAAI;QAC5B,IAAI,CAACX,YAAW,GAAIS,KAAK,CAACG,OAAO;QACjCL,KAAK,CAAC,IAAI,CAACP,YAAY,CAAC;QACxB;MACJ,CAAC,CAAC;IACJ,CAAC;IACFa,KAAKA,CAAEhB,KAAK,EAAEC,QAAO,EAAI;MACvB,MAAMK,IAAG,GAAIb,OAAO,CAAC,CAAC;MACrBG,0BAA0B,CAACU,IAAI,EAAEN,KAAK,EAAEC,QAAQ,EAC/CM,IAAI,CAAEC,cAAc,IAAK;QACtBE,KAAK,CAAC,mBAAmB,CAAC;QAC1B;QACA,MAAMD,IAAG,GAAID,cAAc,CAACC,IAAI;QAChC;MACA,CAAC,EACJE,KAAK,CAAEC,KAAK,IAAK;QAClB,MAAMC,SAAQ,GAAID,KAAK,CAACE,IAAI;QAC5B,IAAI,CAACX,YAAW,GAAIS,KAAK,CAACG,OAAO;QACjCL,KAAK,CAAC,IAAI,CAACP,YAAY,CAAC;MACxB,CAAC,CAAC;IACF,CAAC;IACJc,OAAMA,CAAA,EAAK;MACT,MAAMX,IAAG,GAAIb,OAAO,CAAC,CAAC;MACvBC,OAAO,CAACY,IAAI,CAAC,CAACC,IAAI,CAAC,MAAM;QACvBG,KAAK,CAAC,iCAAiC,CAAC;MAC1C,CAAC,CAAC,CAACC,KAAK,CAAEC,KAAK,IAAK,CACpB,CAAC,CAAC;IACH;EACF,CAAC;EACAM,QAAQ,EAAE;IACV,GAAGrB,UAAU,CAAC,CAAC,eAAe,CAAC;EACjC;AACF","ignoreList":[]}]}